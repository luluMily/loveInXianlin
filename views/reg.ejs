<% include header.ejs %>
        
<form class="form-horizontal" method="post">
<fieldset>
<legend>User Registration</legend>
<div class="form-group">
    <label for="username" class="col-sm-2 control-label">Username</label>
    <div class="col-sm-4">
      <input type="text" class="form-control" id="username" name="username" placeholder="Username">
      <p class="help-block">Your Account Number</p>
    </div>
  </div>
  <div class="form-group">
    <label for="password" class="col-sm-2 control-label">密码</label>
    <div class="col-sm-4">
      <input type="password" class="form-control" id="password" name="password" placeholder="密码">
    </div>
  </div>
  <div class="form-group">
    <label for="password-repeat" class="col-md-2 control-label">确认密码</label>
    <div class="col-md-4">
      <input type="password" class="form-control" id="password-repeat" name="password-repeat" placeholder="确认密码">
    </div>
  </div>
	<div class="form-group">
    <label for="password-repeat" class="col-md-2 control-label">微信号</label>
    <div class="col-md-4">
      <input type="text" class="form-control" id="wechat" name="wechat" placeholder="微信号(建议使用小号)">
    </div>
  </div>
	<div class="form-group">
    <label for="password-repeat" class="col-md-2 control-label">QQ</label>
    <div class="col-md-4">
      <input type="text" class="form-control" id="qq" name="qq" placeholder="QQ(建议使用小号)">
    </div>
  </div>
	<div class="form-group">
    <label for="password-repeat" class="col-md-2 control-label">无图无真相</label>
    <div class="col-md-4">
      <input type="file" class="form-control" id="pic" name="pic">
    </div>
  </div>

<div class="form-group" >
  <div class="col-sm-offset-2 col-sm-10">
<button type="submit" class="btn btn-primary">Submit</button>
  </div>
</div>
</fieldset>
</form>

<% include footer.ejs %>
